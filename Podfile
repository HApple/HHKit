source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'

platform :ios, '9.0'

# 去掉由pod引入的第三方库的警告，需要pod install命令才生效
inhibit_all_warnings!

target 'HHKit' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for HHKit
    pod 'YBModelFile', '~> 1.0.3'
    pod 'AFNetworking'
    pod 'Masonry'
    pod 'MJRefresh'
    pod 'YYKit'
end
